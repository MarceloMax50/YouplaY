
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <title>Escola de música YouplaY</title>
    @*<link rel="stylesheet" href="~/css/site.css" />*@
    <link rel="icon" href="~/Imagens/favicon.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/Imagens/apple-icon.png" />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/fontawesome/css/all.css" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/now-ui-kit.css" />

</head>
<body class="index-page sidebar-collapse">
    <nav class="navbar navbar-expand-lg bg-primary fixed-top navbar-transparent" color-on-scroll="400">
        <div class="container">
            <div class="navbar-translate">
                <a class="navbar-brand" href="/Home/Index" data-placement="bottom">
                    YouplaY
                </a>
                <button class="navbar-toggler navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-bar top-bar"></span>
                    <span class="navbar-toggler-bar middle-bar"></span>
                    <span class="navbar-toggler-bar bottom-bar"></span>
                </button>
            </div>
            <div id="inicio">
                <div class="collapse navbar-collapse justify-content-end" id="navigation" data-nav-image="../../Imagens/blurred-image-1.jpg">
                    <ul class="navbar-nav">
                        <li class="nav-item mr-3">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item mr-3">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="About">Sobre</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link btn btn-neutral" asp-area="" asp-controller="Login" asp-action="Index">
                                <i class="far fa-user"></i>
                                Login
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <!-- End Navbar -->
    <div class="wrapper">
        <div class="page-header clear-filter" filter-color="orange">
            <div class="page-header-image" data-parallax="true" style="background-image:url('../../Imagens/001img.jpg');">
            </div>
            <div class="container">
                <div class="content-center brand">
                    <img src="~/Imagens/Saxlogo.png" width='200' height='200'>
                    <h1 class="h1-seo">YouplaY</h1>
                    <h3>A melhor escola de música do Brasil.</h3>
                </div>

            </div>
        </div>
        <div class="section section-signup" style="background-image: url('../../Imagens/006img.jpg'); background-size: cover; background-position: top center; min-height: 700px;">
            <div class="container">
                <div class="row">
                    <div class="card">
                        <div class="card-header">
                            <ul class="nav nav-tabs nav-tabs-neutral justify-content-center" role="tablist" data-background-color="orange">
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" role="tab">
                                        Confira Nossos Cursos
                                    </a>
                                </li>
                            </ul>

                        </div>
                    </div>
                    <div class="row justify-content-center">
                        <div class="col-lg-12 col-md-12">
                            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                <ol class="carousel-indicators">
                                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                </ol>
                                <div class="carousel-inner" role="listbox">
                                    <div class="carousel-item active">
                                        <img class="d-block" src="https://vsoschoolofmusic.ca/drive/uploads/2017/10/Suzuki-prep-classes-mh-1600x820.jpg" alt="First slide">
                                        <div class="carousel-caption d-none d-md-block">
                                            <h5>Aulas de Violino</h5>
                                        </div>
                                    </div>
                                    <div class="carousel-item">
                                        <img class="d-block" src="https://media.washtimes.com/media/image/2016/12/04/exchange_guitar_class.jpeg" alt="Second slide">
                                        <div class="carousel-caption d-none d-md-block">
                                            <h5>Aulas de Violão</h5>
                                        </div>
                                    </div>
                                    <div class="carousel-item">
                                        <img class="d-block" src="https://www.dailydot.com/wp-content/uploads/e7b/94/5ccfc8f4cec4d024ea42aa559446b572.jpg" alt="Third slide">
                                        <div class="carousel-caption d-none d-md-block">
                                            <h5>Aulas de Piano</h5>
                                        </div>
                                    </div>
                                </div>
                                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                    <i class="now-ui-icons arrows-1_minimal-left"></i>
                                </a>
                                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                    <i class="now-ui-icons arrows-1_minimal-right"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>



        <footer class="footer" data-background-color="black">
            <div class=" container ">
                <nav>
                    <ul>

                        <li>
                            <a href="">
                                Sobre Nós
                            </a>
                        </li>

                    </ul>
                </nav>
                <div class="copyright" id="copyright">
                    &copy;
                    <script>
                        document.getElementById('copyright').appendChild(document.createTextNode(new Date().getFullYear()))
                    </script>, Designed by
                    <a href="" target="_blank">Marcelo & Thiago </a>. Coded by
                    <a href="" target="_blank">Marcelo & Thiago</a>.
                </div>
            </div>
        </footer>
    </div>
    <script type="text/javascript" src="~/js/core/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/core/popper.min.js"></script>
    <script type="text/javascript" src="~/js/core/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/plugins/bootstrap-switch.js"></script>
    <script type="text/javascript" src="~/js/plugins/nouislider.min.js"></script>
    <script type="text/javascript" src="~/js/plugins/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="~/js/now-ui-kit.js?v=1.3.0"></script>

</body>
</html>
